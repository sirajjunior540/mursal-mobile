{
  "name": "DriverAppNew",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "start": "react-native start",
    "test": "jest",
    "pod-install": "cd ios && pod install",
    "clean": "cd android && ./gradlew clean && cd ../ios && xcodebuild clean && cd ..",
    "clean:all": "npm run clean && watchman watch-del-all && rm -rf node_modules && npm install && npm run pod-install",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "build:ios": "cd ios && xcodebuild -workspace DriverAppNew.xcworkspace -scheme DriverAppNew -configuration Release -destination generic/platform=iOS archive -archivePath ./build/DriverAppNew.xcarchive",
    "prepare:release": "npm run lint:fix && npm run typecheck && npm run test",
    "release:android": "npm run prepare:release && npm run build:android",
    "release:ios": "npm run prepare:release && npm run build:ios",
    "setup:env": "node scripts/setup-env.js",
    "setup:ip": "node scripts/setup-env.js --ip",
    "setup:cloudflare": "node scripts/setup-cloudflare.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^22.2.1",
    "@react-native-firebase/messaging": "^22.2.1",
    "@react-native-picker/picker": "^2.6.1",
    "@react-native/new-app-screen": "0.80.0",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/native-stack": "^7.3.21",
    "@react-navigation/stack": "^7.4.2",
    "expo-font": "^11.10.2",
    "i18next": "^25.3.2",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-i18next": "^15.6.0",
    "react-native": "0.80.0",
    "react-native-config": "^1.5.5",
    "react-native-device-info": "^14.0.4",
    "react-native-gesture-handler": "^2.26.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-keychain": "^10.0.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.4.2",
    "react-native-maps": "^1.14.0",
    "react-native-permissions": "^5.4.1",
    "react-native-safe-area-context": "^5.5.0",
    "react-native-screens": "^4.11.1",
    "react-native-sound": "^0.11.2",
    "react-native-vector-icons": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.0",
    "@react-native/eslint-config": "0.80.0",
    "@react-native/metro-config": "0.80.0",
    "@react-native/typescript-config": "0.80.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
